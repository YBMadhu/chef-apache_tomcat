# setenv.sh
#
# Generated by Chef for <%= node['fqdn'] %>
# Local modifications will be overwritten.
#
<% if @additional %>
  <% @additional.each do |item| %>
<%= item %>
  <% end %>
<% end %>
<% if @catalina_out_dir %>
CATALINA_OUT="<%= @catalina_out_dir %>/catalina.out"
<% end %>
<% if @java_home %>
JAVA_HOME="<%= @java_home %>"
<% end %>
<% unless @java_opts.nil? || @java_opts.empty? %>
JAVA_OPTS="<%= @java_opts %>"
<% end %>
<% unless @catalina_opts.nil? || @catalina_opts.empty? %>
CATALINA_OPTS="<%= @catalina_opts %>"
<% end %>
